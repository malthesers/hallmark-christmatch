{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 160,
  "trailingComma": "none",
  "overrides": [
    {
      "files": ["**.ts"],
      "options": {
        "printWidth": 250
      }
    },
    {
      "files": [ "src/components/icons/**.vue"],
      "options": {
        "printWidth": 800
      }
    }
  ]
}